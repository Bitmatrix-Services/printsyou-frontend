{
  "extends": "next/core-web-vitals",
  "rules": {
    "no-var": "error",
    "react/no-unescaped-entities": "off",
    "no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_",
        "vars": "all",
        "args": "after-used",
        "ignoreRestSiblings": false
      }
    ]
  }
}
